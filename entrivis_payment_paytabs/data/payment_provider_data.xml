<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="payment_acquirer_paytabs_val" model="payment.method">
            <field name="name">paytabs</field>
            <field name="code">paytabs</field>
            <field name="active">True</field>
            <field name="image" type="base64" file="entrivis_payment_paytabs/static/src/img/paytabs_icon.png"/>
            <field name="support_tokenization">False</field>
            <field name="support_express_checkout">False</field>
            <field name="support_refund">partial</field>
        </record>

        <record id="payment_acquirer_paytabs" model="payment.provider">
            <field name="name">PayTabs</field>
            <field name="code">paytabs</field>
            <field name="redirect_form_view_id" ref="redirect_form"/>
            <field name="image_128" type="base64"
                   file="entrivis_payment_paytabs/static/src/img/paytabs_icon.png"/>
            <field name="company_id" ref="base.main_company"/>
            <field name="module_id" ref="base.module_entrivis_payment_paytabs"/>
            <field name="payment_method_ids"
                   eval="[Command.set([
                   ref('payment_acquirer_paytabs_val'),
               ])]"
            />
        </record>

    </data>
</odoo>
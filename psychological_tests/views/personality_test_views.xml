<odoo>
  <record id="view_psychological_test_form" model="ir.ui.view">
    <field name="name">psychological.test.form</field>
    <field name="model">psychological.test</field>
    <field name="arch" type="xml">
      <form string="Psychological Test ">
        <sheet>
          <group>
            <field name="name" />
            <field name="description" />
            <field name="image" widget="image" class="oe_avatar" />
            <field name="website_published" />
          </group>
        </sheet>
      </form>
    </field>
  </record>

  <record id="view_psychological_test_tree" model="ir.ui.view">
    <field name="name">psychological.test.tree</field>
    <field name="model">psychological.test</field>
    <field name="arch" type="xml">
      <tree>
        <field name="name" />
        <field name="website_published" />
      </tree>
    </field>
  </record>

  <record id="action_psychological_test" model="ir.actions.act_window">
    <field name="name">Psychological Tests</field>
    <field name="res_model">psychological.test</field>
    <field name="view_mode">tree,form</field>
  </record>

</odoo>

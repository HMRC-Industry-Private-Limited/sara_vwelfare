<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <record id="view_test_type_result_line_tree" model="ir.ui.view">
    <field name="name">test.type.result.line.tree</field>
    <field name="model">test.type.result.line</field>
    <field name="arch" type="xml">
      <tree>
        <field name="test_id" />
        <field name="min_score" />
        <field name="max_score" />
        <field name="result_class" />
      </tree>
    </field>
  </record>

  <record id="view_test_type_result_line_form" model="ir.ui.view">
    <field name="name">test.type.result.line.form</field>
    <field name="model">test.type.result.line</field>
    <field name="arch" type="xml">
      <form>
        <group>
          <field name="test_id" />
          <field name="min_score" />
          <field name="max_score" />
          <field name="result_class" />
          <field name="result_text" />
        </group>
      </form>
    </field>
  </record>


  <record id="action_test_result_line" model="ir.actions.act_window">
    <field name="name">Test Result</field>
    <field name="res_model">test.type.result.line</field>
    <field name="view_mode">tree,form</field>
  </record>
</odoo>


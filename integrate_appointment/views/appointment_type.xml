<odoo>
    <record id="view_calendar_type_form" model="ir.ui.view">
        <field name="name">appointment.type.form.inherit</field>
        <field name="model">appointment.type</field>
        <field name="inherit_id" ref="appointment.appointment_type_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='staff_user_ids']" position="after">
                <field name="duration_minutes"/>
                <field name="doctor_ids" string="Doctors">
                    <tree>
                        <field name="name"/>
                        <field name="doctor_id"/>  
                    </tree>
                </field>
            </xpath>
        </field>
    </record>
</odoo>
